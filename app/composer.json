{
    "name": "kossan_rest_api",
    "description": "RESTful API for Kossan",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/",
            "Utils\\": "utils/"
        }
    },
    "authors": [
        {
            "name": "Kossan Rubber Industries Bhd"
        }
    ],
    "require": {
        "php": ">=8.2",
        "vlucas/phpdotenv": "^5.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}